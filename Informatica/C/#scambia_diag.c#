#include <stdio.h>
int d;

void stampamat(int m[][d]){
    int i,j;
    for(i=0;i<d;i++){
        for(j=0;j<d;j++){
            printf("%d ",m[j][i]);
        }
        printf("\n");
    }
}


void insmat(int m[][d]){
    int i,j;
    for(i=0;i<d;i++){
        for(j=0;j<d;j++){
            scanf("%d",&m[i][j]);
        }
    }
}




int main(){
  printf("grandezza matrice");
  scanf("%d",&d);
  int mat[d][d];
  insmat(mat);
  stampamat(mat);



}
